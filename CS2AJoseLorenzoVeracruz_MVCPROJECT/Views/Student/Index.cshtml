@model IEnumerable<CS2AJoseLorenzoVeracruz_MVCPROJECT.BusLogic.Model.tblStudent>

<div>
    <h1>Student List</h1>
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>Course</th>
        </tr>

        
            @foreach(var students in Model)
            {
            <tr>
                <td>@students.FirstName</td>
                <td>@students.LastName</td>
                <td>@students.Age</td>
                <td>@students.Course</td>
            </tr>
            }
        
    </table>
</div>